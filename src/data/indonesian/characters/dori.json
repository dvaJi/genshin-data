  "passives": [
    {
      "id": "an_eye_for_gold",
      "name": "An Eye for Gold",
      "description": "Setelah Karakter yang terhubung dengan <span>Jinni</span> memicu Electro-Charged, Superconduct, Overloaded, Quicken, Aggravate, Hyperbloom, atau reaksi Swirl atau Crystallize dari Electro, maka CD <span>Spirit-Warding Lamp: Troubleshooter Cannon</span> berkurang 1 detik.<br/> Efek ini dapat terpicu setiap 3 detik.",
      "level": 1
    },
    {
      "id": "compound_interest",
      "name": "Compound Interest",
      "description": "Saat Troubleshooter Shot atau After-Sales Service Round dari <span>Spirit-Warding Lamp: Troubleshooter Cannon</span> mengenai musuh, Dori akan memulihkan 5 Energy untuk setiap 100% Energy Recharge yang dimiliki Dori.<br/>Untuk setiap Spirit-Warding Lamp: Troubleshooter Cannon, hanya satu pemulihan Energi yang dapat terpicu dan maksimal 15 Energy yang bisa dipulihkan dengan cara ini.",
      "level": 4
    },
    {
      "id": "unexpected_order",
      "name": "Unexpected Order",
      "description": "Saat membuat Material Penguatan Senjata dan Karakter, memiliki 25% persentase untuk mendapatkan kembali sebagian material yang digunakan.",
      "level": 0
    }
  ],
