  "passives": [
    {
      "id": "spotless_waters",
      "name": "Spotless Waters",
      "description": "After the Dewdrop fired by the Hold Mode of the <span>Aquacrest Saber</span> hits an opponent, a Sourcewater Droplet will be generated near to the Traveler. If the Traveler picks it up, they will restore 7% HP.<br/>1 Droplet can be created this way every second, and each use of Aquacrest Saber can create 4 Droplets at most.",
      "level": 1
    },
    {
      "id": "clear_waters",
      "name": "Clear Waters",
      "description": "If HP has been consumed via Suffusion while using the Hold Mode <span>Aquacrest Saber</span>, the Torrent Surge at the skill's end will deal Bonus DMG equal to 45% of the total HP the Traveler has consumed in this skill use via Suffusion.<br/>The maximum DMG Bonus that can be gained this way is 5,000.",
      "level": 4
    }
  ],
