  "passives": [
    {
      "id": "agony_alone_may_be_repaid",
      "name": "Agony Alone May Be Repaid",
      "description": "<span>Blood-Debt Directives</span> have the following characteristics:<br/>·Arlecchino will be granted a Bond of Life worth 130% of her Max HP when an opponent to which she herself applied a Directive is defeated.<br/>·5s after a Directive is applied, it will be upgraded to a Blood-Debt Due. When absorbed, it will instead grant Arlecchino a Bond of Life worth 130%.<br/>A Bond of Life created in the aforementioned ways cannot exceed the original limit on the value of Bonds of Life obtained through <span>All Is Ash</span>.",
      "level": 1
    },
    {
      "id": "strength_alone_can_defend",
      "name": "Strength Alone Can Defend",
      "description": "Arlecchino gains 1% All Elemental and Physical RES for every 100 ATK she has in excess of 1,000. The maximum RES increase she can gain this way for each is 20%.",
      "level": 4
    },
    {
      "id": "the_balemoon_alone_may_know",
      "name": "The Balemoon Alone May Know",
      "description": "While in combat, Arlecchino gains a 40% <span>Pyro DMG Bonus</span> and can only be healed through <span>Balemoon Rising</span>.",
      "level": 0
    }
  ],
  "constellations": [
    {
      "_id": 961,
      "id": "all_reprisals_and_arrears_mine_to_bear",
      "name": "\"All Reprisals and Arrears, Mine to Bear...\"",
      "description": "Masque of the Red Death is further enhanced, the value of the increase is 100%. Additionally, Arlecchino's interruption resistance is increased when she performs Normal or Charged Attacks while affected by the Masque of the Red Death.",
      "level": 1
    },
    {
      "_id": 962,
      "id": "all_rewards_and_retribution_mine_to_bestow",
      "name": "\"All Rewards and Retribution, Mine to Bestow...\"",
      "description": "<span>Blood-Debt Directives</span> are now already Blood-Debt Due when first applied.<br/>When Arlecchino absorbs such a Due, she unleashes Balemoon Bloodfire in front of her, dealing 900% of her ATK as <span>AoE Pyro DMG</span> and increasing her All Elemental RES and Physical RES by 20% for 15s. This effect can trigger once every 10s.<br/>You must first unlock the Passive Talent \"Agony Alone May Be Repaid.\"",
      "level": 2
    },
    {
      "_id": 963,
      "id": "you_shall_become_a_new_member_of_our_family",
      "name": "\"You Shall Become a New Member of Our Family...\"",
      "description": "Increases the Level of <span>Normal Attack: Invitation to a Beheading</span> by 3.<br/>Maximum upgrade level is 15.",
      "level": 3
    },
    {
      "_id": 964,
      "id": "you_shall_love_and_protect_each_other_henceforth",
      "name": "\"You Shall Love and Protect Each Other Henceforth...\"",
      "description": "When Arlecchino successfully absorbs a Blood-Debt Directive, <span>Balemoon Rising</span>'s CD will decrease by 2s and 15 Elemental Energy will be restored to her. This effect can occur once every 10s.",
      "level": 4
    },
    {
      "_id": 965,
      "id": "for_alone_we_are_as_good_as_dead",
      "name": "\"For Alone, We Are as Good as Dead...\"",
      "description": "Increases the Level of <span>Balemoon Rising</span> by 3.<br/>Maximum upgrade level is 15.",
      "level": 5
    },
    {
      "_id": 966,
      "id": "from_this_day_on_we_shall_delight_in_new_life_together",
      "name": "\"From This Day On, We Shall Delight in New Life Together.\"",
      "description": "The DMG of <span>Balemoon Rising</span> is increased by Arlecchino's ATK multiplied by 700% of Arlecchino's current Bond of Life percentage.<br/>For 20s after Arlecchino uses <span>All Is Ash</span>, both her Normal Attacks and Elemental Burst gain 10% increased CRIT Rate and 70% increased CRIT DMG. This effect can be triggered up to once every 15s.",
      "level": 6
    }
