{
  "version": "0.16.0",
  "license": "MIT",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "files": [
    "dist",
    "src/index.ts",
    "src/types"
  ],
  "engines": {
    "node": ">=13"
  },
  "scripts": {
    "build": "webpack",
    "generate": "node ./scripts/generate.js",
    "check-missing": "npm run generate && npm run build && node ./scripts/check-missing.js",
    "format-data": "prettier --loglevel warn --write \"src/data/**/*.json\"",
    "prepare": "npm run generate && npm run build"
  },
  "prettier": {
    "printWidth": 80,
    "semi": true,
    "singleQuote": true,
    "trailingComma": "es5"
  },
  "name": "genshin-data",
  "author": "dvaJi",
  "module": "dist/.esm.js",
  "devDependencies": {
    "prettier": "^2.5.1",
    "ts-node": "^10.4.0",
    "tslib": "^2.3.1",
    "ts-loader": "^9.2.6",
    "typescript": "^4.5.4",
    "webpack": "^5.65.0",
    "webpack-cli": "^4.9.1"
  }
}
